package problem5;

public class SmallestMultiple {
	public static void main(String args[]) {
		int num = 20;
		int lcmLast = 0;
		System.out.println(gcd(21,9));
		for(int i = 1; i<21; i++) {
			
		}
	}
	
	public static int gcd(int num1, int num2) {
		int remainder = 0;
		while(num1%num2!=0) {
			remainder = num1%num2;
			num1=num2;
			num2=remainder;
		}
		return num2;
	}
}
